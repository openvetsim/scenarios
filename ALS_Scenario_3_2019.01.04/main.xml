<?xml version='1.0' encoding='UTF-8'?>
<scenario>
	<header>
		<author>Daniel J. Fletcher, PhD, DVM, DACVECC; Kenichiro Yagi, MS, RVT, VTS (ECC, SAIM)</author>
		<title>
			<name>ALS Scenario 3</name>
			<top>5</top>
			<left>10</left>
		</title>
		<date_of_creation>2018-10-03</date_of_creation>
		<description>Scenario depicting cardiac arrest due to GDV</description>
	</header>
	<profile>
		<avatar>
			<filename>stock-dog.jpg</filename>
			<height_pct>100</height_pct>
			<width_pct>100</width_pct>
		</avatar>
		<summary>
			<description>
				Pocholo is a 5 year old castrated male Great Dane that presented to your clinic with a 3-hour history of restlessness, panting, pacing, and unproductive retching. His abdomen was distended and tympanic on presentation. You placed an IV catheter, administered 0.1 mg/kg hydromorphone and bolused 2 liters of 0.9% NaCl over 15 minutes.  You trocharized to decompress his stomach and are reassessing after trocharization to prepare him for radiographs.
			</description>
			<breed>Great Dane</breed>
			<sex>Male Castrated</sex>
			<weight>50 kg</weight>
			<complaint>Restlessness, panting, pacing, and unproductive retching</complaint>
			<image>pocholo.jpg</image>
			<species>Canine</species>
		</summary>
		<controls>
			<color>#000000</color>
			<control>
				<title>Vocalizations</title>
				<id>vocals-dog-control</id>
				<top>125</top>
				<left>300</left>
			</control>
			<control>
				<title>Right Lung Sounds</title>
				<id>right-lung-dog-control</id>
				<top>180</top>
				<left>80</left>
			</control>
			<control>
				<title>Left Lung Sounds</title>
				<id>left-lung-dog-control</id>
				<top>180</top>
				<left>320</left>
			</control>
			<control>
				<title>Left Femoral Pulse</title>
				<id>left-femoral-pulse-dog-control</id>
				<top>275</top>
				<left>320</left>
			</control>
			<control>
				<title>Right Femoral Pulse</title>
				<id>right-femoral-pulse-dog-control</id>
				<top>275</top>
				<left>75</left>
			</control>
			<control>
				<title>Left Dorsal Pulse</title>
				<id>left-dorsal-pulse-dog-control</id>
				<top>310</top>
				<left>325</left>
			</control>
			<control>
				<title>Right Dorsal Pulse</title>
				<id>right-dorsal-pulse-dog-control</id>
				<top>310</top>
				<left>85</left>
			</control>
			<control>
				<title>Heart Sounds</title>
				<id>heart-sound-dog-control</id>
				<top>225</top>
				<left>221</left>
			</control>
			<control>
				<title>Chest Movement</title>
				<id>chest-dog-control</id>
				<top>210</top>
				<left>325</left>
			</control>
			<control>
				<title>CPR</title>
				<id>button-cpr</id>
				<top>20</top>
				<left>450</left>
			</control>
			<control>
				<title>ECG</title>
				<id>button-ekg</id>
				<top>350</top>
				<left>0</left>
			</control>
			<control>
				<title>SpO&lt;sub&gt;2&lt;&#47;sub&gt;</title>
				<id>button-SpO2</id>
				<top>350</top>
				<left>55</left>
			</control>
			<control>
				<title>ETCO&lt;sub&gt;2&lt;&#47;sub&gt;</title>
				<id>button-CO2</id>
				<top>350</top>
				<left>395</left>
			</control>
			<control>
				<title>Cuff</title>
				<id>button-bpcuff</id>
				<top>350</top>
				<left>450</left>
			</control>
			<control>
				<title>Palpate</title>
				<id>button-palpate</id>
				<top>20</top>
				<left>390</left>
			</control>
			<control>
				<title>Temp</title>
				<id>button-Tperi</id>
				<top>350</top>
				<left>110</left>
			</control>
		</controls>
	</profile>

	<vocals>
		<file>
			<filename>AggressiveDogGrowlBark.wav</filename>
			<title>Dog Bark Growl</title>
		</file>
		<file>
			<filename>BigDogBark.wav</filename>
			<title>Dog Big Bark</title>
		</file>
		<file>
			<filename>DogBarkSnarl.wav</filename>
			<title>Dog Bark Snarl</title>
		</file>
	</vocals>	
	
	<media>
		<file>
			<!-- None -->
		</file>
	</media>

	<init>
		<cardiac>
			<rhythm>vtach1</rhythm>
			<vpc>none</vpc>
			<pea>0</pea>
			<vpc_freq>0</vpc_freq>
			<vfib_amplitude>low</vfib_amplitude>
			<rate>200</rate>
			<bps_sys>70</bps_sys>
			<bps_dia>40</bps_dia>
			<nibp_rate>200</nibp_rate>
			<pulse_strength>weak</pulse_strength>
			<heart_sound>normal</heart_sound>
			<heart_sound_volume>7</heart_sound_volume>
			<ecg_indicator>0</ecg_indicator>
			<arrest>0</arrest>
		</cardiac>
		<respiration>
			<left_lung_sound>normal</left_lung_sound>
			<left_lung_sound_volume>9</left_lung_sound_volume>
			<right_lung_sound>normal</right_lung_sound>
			<right_lung_sound_volume>9</right_lung_sound_volume>
			<spo2>95</spo2>
			<spo2_indicator>0</spo2_indicator>
			<etco2>31</etco2>
			<etco2_indicator>0</etco2_indicator>
			<rate>45</rate>
			<chest_movement>1</chest_movement>
		</respiration>			
		<general>
			<temperature>999</temperature>
		</general>
		<initial_scene>1</initial_scene>
		<record>1</record>
	</init>
   
	<events>
		<category>
			<name>drugs</name>
			<title>Drugs</title>
			<event>
				<title>Low Dose Epi</title>
				<id>epi_low</id>
				<priority>1</priority>
			</event>
			<event>
				<title>High Dose Epi</title>
				<id>epi_high</id>
				<priority>0</priority>
			</event>
			<event>
				<title>Vasopressin</title>
				<id>vasopressin</id>
				<priority>1</priority>
			</event>
			<event>
				<title>Reversal</title>
				<id>reversal</id>
				<priority>1</priority>
			</event>
			<event>
				<title>Fluids</title>
				<id>fluids</id>
				<priority>0</priority>
			</event>
			<event>
				<title>Lidocaine</title>
				<id>lidocaine</id>
				<priority>0</priority>
			</event>
		</category>
		<category>
			<name>interventions</name>
			<title>Interventions</title>
			<event>
				<title>Defib</title>
				<id>aed</id>
				<priority>1</priority>
			</event>
			<event>
				<title>Terminal</title>
				<id>terminal</id>
				<priority>1</priority>
			</event>
		</category>
	</events>
	
	<scene>
		<!-- Starting scene -->
		<title>Initial State</title>
		<id>1</id>
		<init>
		<cardiac>
		</cardiac>
		<respiration>
		</respiration>			
		<general>
		</general>
		</init>
		
		<!-- triggers to leave the scene -->
		<!-- timeout to Vfib 1 in 3 minutes if no lidocaine -->
		<timeout>
			<timeout_value>180</timeout_value>
			<scene_id>4</scene_id>
		</timeout>
		<triggers>
			<!-- jump to lidocaine administration if lidocaine given -->
			<trigger>
				<event_id>lidocaine</event_id>
				<scene_id>2</scene_id>
			</trigger>
		</triggers>
	</scene>
	
	<scene>
		<!-- Lidocaine Administration -->
		<title>Lidocaine admin - delay to sinus</title>
		<id>2</id>
		<init>
		</init>
		
		<!-- triggers to leave the scene -->
		<!-- After 30 seconds, convert to sinus -->
		<timeout>
			<timeout_value>30</timeout_value>
			<scene_id>3</scene_id>
		</timeout>
		<triggers>
			<!-- jump to terminal asystole if defib attempted -->
			<trigger>
				<event_id>aed</event_id>
				<scene_id>100</scene_id>
			</trigger>	
		</triggers>
	</scene>

	<scene>
		<!-- Lidocaine Administration -->
		<title>Lidocaine admin - Wait for Vfib</title>
		<id>3</id>
		<init>
			<cardiac>
				<rhythm>sinus</rhythm>
				<rate>120</rate>
				<bps_sys>74</bps_sys>
				<bps_dia>42</bps_dia>
				<nibp_rate>118</nibp_rate>
				<transfer_time>15</transfer_time>
			</cardiac>
		</init>
		
		<!-- triggers to leave the scene -->
		<!-- After 45 seconds, progress to vfib -->
		<timeout>
			<timeout_value>45</timeout_value>
			<scene_id>4</scene_id>
		</timeout>
		<triggers>
			<!-- jump to terminal asystole if defib attempted -->
			<trigger>
				<event_id>terminal</event_id>
				<scene_id>100</scene_id>
			</trigger>			
		</triggers>
	</scene>
	
	<scene>
		<!--Vfib 1 - Need Defibrillation -->
		<title>Vfib 1 - Need defibrillation</title>
		<id>4</id>
		<init>			
			<cardiac>
				<vfib_amplitude>med</vfib_amplitude>
				<rhythm>vfib</rhythm>
				<rate>0</rate>
				<bps_sys>0</bps_sys>
				<bps_dia>0</bps_dia>
				<nibp_rate>0</nibp_rate>
				<pulse_strength>none</pulse_strength>
				<heart_sound>normal</heart_sound>
				<heart_sound_volume>0</heart_sound_volume>
				<arrest>1</arrest>
				<transfer_time>0</transfer_time>
			</cardiac>
			<respiration>
				<left_lung_sound>normal</left_lung_sound>
				<left_lung_sound_volume>0</left_lung_sound_volume>
				<right_lung_sound>normal</right_lung_sound>
				<right_lung_sound_volume>0</right_lung_sound_volume>
				<spo2>0</spo2>
				<spo2_indicator>0</spo2_indicator>
				<etco2>16</etco2>
				<etco2_indicator>0</etco2_indicator>
				<rate>0</rate>
				<chest_movement>0</chest_movement>
				<transfer_time>0</transfer_time>
			</respiration>		
		</init>
		
		<!-- triggers to leave the scene -->
		<!-- After 3 minutes without defibrillation, progress to terminal asystole -->
		<timeout>
			<timeout_value>180</timeout_value>
			<scene_id>100</scene_id>
		</timeout>

		<triggers>
			<!-- jump to Vfib 2 if defibrillation is performed -->
			<trigger>
				<event_id>aed</event_id>
				<scene_id>5</scene_id>
			</trigger>
		</triggers>
	</scene>
	
	<scene>
		<!-- Vfib 2 - Need Compressions -->
		<title>Vfib 2 - Need compressions</title>
		<id>5</id>
				
		<!-- triggers to leave the scene -->
		<triggers>
			<!-- jump to Vfib 3 if compressions for at least 100 seconds -->
			<trigger>
				<test>GTE</test>
				<scene_id>6</scene_id>
				<cpr>
					<duration>100</duration>
				</cpr>
			</trigger>					
		</triggers>
	</scene>
	
	<scene>
		<!-- Vfib 3 - Need defibrillation -->
		<title>Vfib 3 - Need defibrillation</title>
		<id>6</id>
				
		<!-- After 2.5 minutes without defibrillation, progress to terminal asystole -->
		<timeout>
			<timeout_value>150</timeout_value>
			<scene_id>100</scene_id>
		</timeout>
			
		<!-- triggers to leave the scene -->
		<triggers>
			<!-- jump to Vfib 4 if defibrillation is performed -->
			<trigger>
				<event_id>aed</event_id>
				<scene_id>7</scene_id>
			</trigger>
		</triggers>
	</scene>
	
	<scene>
		<!-- Vfib 4 - Need Compressions -->
		<title>Vfib 4 - Need comps</title>
		<id>7</id>
				
		<!-- triggers to leave the scene -->
		<triggers>
			<!-- jump to Vfib 3 if compressions for at least 100 seconds -->
			<trigger>
				<test>GTE</test>
				<scene_id>8</scene_id>
				<cpr>
					<duration>100</duration>
				</cpr>
			</trigger>					
		</triggers>
	</scene>
	
	<scene>
		<!-- Vfib 5 - Need defibrillation -->
		<title>Vfib 5 - Need defibrillation</title>
		<id>8</id>
				
		<!-- After 2.5 minutes without defibrillation, progress to terminal asystole -->
		<timeout>
			<timeout_value>150</timeout_value>
			<scene_id>100</scene_id>
		</timeout>
		<!-- triggers to leave the scene -->
		<triggers>
			<!-- jump to Asystole if defibrillation is performed -->
			<trigger>
				<event_id>aed</event_id>
				<scene_id>9</scene_id>
			</trigger>
		</triggers>
	</scene>
	
	<scene>
		<!-- Asystole - Delay to sinus -->
		<title>Asystole - Delay to sinus</title>
		<id>9</id>
		<init>
			<rhythm>asystole</rhythm>
			<transfer_time>0</transfer_time>
		</init>
		
		<!-- After 30 seconds progress to sinus rhythm -->
		<timeout>
			<timeout_value>30</timeout_value>
			<scene_id>10</scene_id>
		</timeout>	
		<!-- triggers to leave the scene -->
		<triggers>
		</triggers>
	</scene>
	
	<scene>
		<!-- ROSC - Sinus Rhythm -->
		<title>ROSC - Sinus Rhythm</title>
		<id>10</id>
		<init>
			<cardiac>
				<rhythm>sinus</rhythm>
				<rate>120</rate>
				<arrest>0</arrest>
				<pulse_strength>strong</pulse_strength>
				<heart_sound>normal</heart_sound>
				<heart_sound_volume>7</heart_sound_volume>
				<pea>0</pea>
				<bps_sys>100</bps_sys>
				<bps_dia>50</bps_dia>
				<nibp_rate>118</nibp_rate>
				<transfer_time>0</transfer_time>
			</cardiac>
			<respiration>
				<left_lung_sound>normal</left_lung_sound>
				<left_lung_sound_volume>9</left_lung_sound_volume>
				<right_lung_sound>normal</right_lung_sound>
				<right_lung_sound_volume>9</right_lung_sound_volume>
				<spo2>94</spo2>
				<rate>36</rate>
				<etco2>55</etco2>
				<transfer_time>0</transfer_time>
				<chest_movement>1</chest_movement>
			</respiration>
			<general>
				<temperature>1003</temperature>
				<transfer_time>0</transfer_time>
			</general>
		</init>
	</scene>
	
	<!-- terminal asystole -->
	<scene>
		<title>Terminal Asystole</title>
		<id>100</id>
		<init>
			<cardiac>
				<arrest>1</arrest>
				<rate>0</rate>
				<bps_sys>0</bps_sys>
				<bps_dia>0</bps_dia>
				<nibp_rate>0</nibp_rate>
				<transfer_rate>0</transfer_rate>
			</cardiac>
		</init>
	</scene>
	
</scenario>