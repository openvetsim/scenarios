	<vocals>
		<file>
			<filename>AggressiveDogGrowlBark.wav</filename>
			<title>Dog Bark Growl</title>
		</file>
		<file>
			<filename>BigDogBark.wav</filename>
			<title>Dog Big Bark</title>
		</file>
		<file>
			<filename>DogBarkSnarl.wav</filename>
			<title>Dog Bark Snarl</title>
		</file>
