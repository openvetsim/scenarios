			<filename>160848403.jpg</filename>
			<title>Lucy Image</title>
		</file>
	</media>

	<init>
		<cardiac>
			<rhythm>sinus</rhythm>
			<vpc>1-1</vpc>
			<pea>0</pea>
			<vpc_freq>52</vpc_freq>
			<vfib_amplitude>low</vfib_amplitude>
			<rate>180</rate>
			<bps_sys>100</bps_sys>
			<bps_dia>60</bps_dia>
			<nibp_rate>180</nibp_rate>
			<pulse_strength>normal</pulse_strength>
			<heart_sound>systolic_murmur</heart_sound>
			<heart_sound_volume>9</heart_sound_volume>
			<ecg_indicator>0</ecg_indicator>
			<arrest>0</arrest>
		</cardiac>
		<respiration>
			<left_lung_sound>coarse_crackles</left_lung_sound>
			<left_lung_sound_volume>9</left_lung_sound_volume>
			<right_lung_sound>coarse_crackles</right_lung_sound>
			<right_lung_sound_volume>9</right_lung_sound_volume>
			<spo2>86</spo2>
			<spo2_indicator>0</spo2_indicator>
			<etco2>45</etco2>
			<etco2_indicator>0</etco2_indicator>
			<rate>50</rate>
			<chest_movement>1</chest_movement>
		</respiration>			
		<general>
			<temperature>990</temperature>
		</general>
		<initial_scene>1</initial_scene>
		<record>1</record>
	</init>
   
	<events>
		<category>
			<name>drugs</name>
			<title>Drugs</title>
			<event>
				<title>Furosemide</title>
				<id>furosemide</id>
				<priority>1</priority>
			</event>
			<event>
				<title>Nitroprusside</title>
				<id>nitroprusside</id>
				<priority>1</priority>
			</event>
		</category>
		<category>
			<name>interventions</name>
			<title>Interventions</title>
			<event>
				<title>Oxygen</title>
				<id>oxygen</id>
				<priority>1</priority>
			</event>
			<event>
				<title>Terminal</title>
				<id>terminal</id>
				<priority>1</priority>
			</event>
		</category>
	</events>
	
	<scene>
		<!-- Starting scene -->
		<title>Initial State</title>
		<id>1</id>
		<init>
			<cardiac>
				<rhythm>sinus</rhythm>
				<vpc>1-1</vpc>
				<pea>0</pea>
				<vpc_freq>52</vpc_freq>
				<vfib_amplitude>low</vfib_amplitude>
				<rate>180</rate>
				<bps_sys>100</bps_sys>
				<bps_dia>60</bps_dia>
				<nibp_rate>180</nibp_rate>
				<pulse_strength>normal</pulse_strength>
				<heart_sound>systolic_murmur</heart_sound>
				<heart_sound_volume>9</heart_sound_volume>
				<ecg_indicator>0</ecg_indicator>
				<arrest>0</arrest>
			</cardiac>
			<respiration>
				<left_lung_sound>coarse_crackles</left_lung_sound>
				<left_lung_sound_volume>9</left_lung_sound_volume>
				<right_lung_sound>coarse_crackles</right_lung_sound>
				<right_lung_sound_volume>9</right_lung_sound_volume>
				<spo2>86</spo2>
				<spo2_indicator>0</spo2_indicator>
				<etco2>45</etco2>
				<etco2_indicator>0</etco2_indicator>
				<rate>50</rate>
				<chest_movement>1</chest_movement>
			</respiration>			
			<general>
				<temperature>990</temperature>
			</general>
		</init>
		
		<!-- triggers to leave the scene -->
		<!-- timeout to Asystole if no interventions -->
		<timeout>
			<timeout_value>240</timeout_value>
			<scene_id>100</scene_id>
		</timeout>
		<triggers>
			<!-- jump to Furosemide First if furosemide given -->
			<trigger>
				<event_id>furosemide</event_id>
				<scene_id>2</scene_id>
			</trigger>
			<!-- jump to Oxygen First if oxygen is given -->
			<trigger>
				<event_id>oxygen</event_id>
				<scene_id>5</scene_id>
			</trigger>
			<trigger>
				<event_id>terminal</event_id>
				<scene_id>100</scene_id>
			</trigger>
		</triggers>
	</scene>
	
	<scene>
		<!-- Waiting for Oxygen or Vasodilator -->
		<title>Furosemide First</title>
		<id>2</id>
		<init>
			<cardiac>
				<rhythm>sinus</rhythm>
				<vpc>1-1</vpc>
				<pea>0</pea>
				<vpc_freq>52</vpc_freq>
				<vfib_amplitude>low</vfib_amplitude>
				<rate>140</rate>
				<bps_sys>110</bps_sys>
				<bps_dia>70</bps_dia>
				<nibp_rate>140</nibp_rate>
				<pulse_strength>normal</pulse_strength>
				<heart_sound>systolic_murmur</heart_sound>
				<heart_sound_volume>9</heart_sound_volume>
				<ecg_indicator>0</ecg_indicator>
				<arrest>0</arrest>
				<transfer_time>30</transfer_time>
			</cardiac>
			<respiration>
				<left_lung_sound>fine_crackles</left_lung_sound>
				<left_lung_sound_volume>9</left_lung_sound_volume>
				<right_lung_sound>fine_crackles</right_lung_sound>
				<right_lung_sound_volume>9</right_lung_sound_volume>
				<spo2>88</spo2>
				<spo2_indicator>0</spo2_indicator>
				<etco2>45</etco2>
				<etco2_indicator>0</etco2_indicator>
				<rate>40</rate>
				<chest_movement>1</chest_movement>
				<transfer_time>30</transfer_time>
			</respiration>			
			<general>
				<temperature>990</temperature>
			</general>		
		</init>
		
		<!-- triggers to leave the scene -->
		<triggers>
			<!-- jump to terminal asystole if defib attempted -->
			<trigger>
				<event_id>Terminal</event_id>
				<scene_id>100</scene_id>
			</trigger>
			
			<!-- jump to O2 Furosemide if oxygen given. Terminal if anything else -->
			<trigger>
				<event_id>oxygen</event_id>
				<scene_id>3</scene_id>
			</trigger>
			<trigger>
				<event_id>terminal</event_id>
				<scene_id>100</scene_id>
			</trigger>

		</triggers>
	</scene>
	
